<div class="container">
  <div class= "album_form">
    <%= render partial: 'admin/new_album'%>
  </div>
  <div class="panel panel-default">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th>Release Date</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <%@albums.each do |album|%>
          <%= render 'albums/album_row', album: album%>
        <%end%>
      </tbody>
    </table>
  </div>
</div>
