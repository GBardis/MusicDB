<div class='container'>
  <div class='row'>
    <%= render 'admin/flash_messages'%>
    <%=render 'categories/index_categories'%>
  </div>
  <div class="row">
    <%=link_to 'Admin Dashboard',admin_dashboard_path%>
  </div>
  <div class="row">
    <% if !@album.nil?%>
      <% @album.each do |album|%>
        <%= album.name%>
        <%= album.ci_lower_bound%>
        <%= album.release_date%>
      <%end%>
    <%end%>
  </div>
</div>
